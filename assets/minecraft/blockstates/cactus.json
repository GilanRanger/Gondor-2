{
    "multipart": [
		{"when": {"age": 0}, "apply": { "model": "cactus_stage0"}},
		{"when": {"age": 1}, "apply": { "model": "cactus_stage1"}},
		{"when": {"age": 2}, "apply": { "model": "cactus_stage2"}},
		{"when": {"age": 3}, "apply": { "model": "cactus_stage3"}},
		{"when": {"age": 4}, "apply": { "model": "cactus_stage4"}},
		{"when": {"age": 5}, "apply": { "model": "cactus_stage5"}},
		{"when": {"age": 6}, "apply": { "model": "cactus_stage6"}},
		{"when": {"age": 7}, "apply": { "model": "cactus_stage6", "y": 90}},
		{"when": {"age": 8}, "apply": { "model": "cactus_stage6", "y": 180}},
		{"when": {"age": 9}, "apply": { "model": "cactus_stage6", "y": 270}},
		{"when": { "OR": [{"age": "10"},{"age":"11"},{"age":"12"},{"age":"13"},{"age":"14"}]}, "apply": { "model": "tree_spruce_middle"}},
		{"when": { "OR": [{"age": "10"},{"age":"13"}]}, "apply": { "model": "tree_spruce_sides", "x": 90}},
		{"when": { "OR": [{"age": "11"},{"age":"13"},{"age":"14"}]}, "apply": { "model": "tree_spruce_sides", "y": 90 }},
		{"when": { "OR": [{"age": "12"},{"age":"13"},{"age":"14"}]}, "apply": { "model": "tree_spruce_sides"}},
		{"when": {"age": 15}, "apply": { "model": "barrel"}}
	]
}
